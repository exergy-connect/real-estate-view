{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "real-estate-view",
  "build": {
    "command": "node scripts/unpack.js", // This is the "Magic" hook
    "watch_dir": "docs"                  // Re-run if master JSON changes
  },
  "main": "src/index.ts",
  "compatibility_date": "2026-02-28",
  "observability": {
    "enabled": true
  },
  "assets": {
    "directory": "docs",
    "binding": "ASSETS"
  },
  "compatibility_flags": [
    "nodejs_compat"
  ]
}
